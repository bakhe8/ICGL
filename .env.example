# Example environment variables for ICGL
# Copy to .env and fill values before running in governance mode

# Required: OpenAI API key for LLM access
OPENAI_API_KEY=

# Local Qdrant storage path (used by QdrantAdapter and RIG)
# Use a directory path for local (embedded) Qdrant
ICGL_QDRANT_PATH=data/qdrant_memory

# Optional: override sessions DB path (SQLite)
ICGL_SESSIONS_DB=data/sessions.db

# Dev: enable Vite dev proxy for frontend
VITE_DEV_PROXY=false
OPENAI_API_KEY=your-openai-api-key-here
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=
DATA_DIR=data/
ICGL_RUNTIME_MODE=development
# AutoBeto Environment Variables
# COPY THIS FILE TO .env AND FILL IN YOUR VALUES

# DEBUG=True
OPENAI_API_KEY=your-key-here

